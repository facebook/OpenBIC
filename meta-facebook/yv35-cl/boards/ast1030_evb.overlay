};

&i2c2 {
	pinctrl-0 = <&pinctrl_i2c2_default>;
	clock-frequency = <I2C_BITRATE_FAST_PLUS>;
	status = "okay";

	ipmb2: ipmb@10 {
		compatible = "aspeed,ipmb";
		reg = <0x20>;
		label = "IPMB_2";
		size = <10>;
#ifdef CONFIG_I2C_IPMB_SLAVE
		status = "okay";
#endif
	};

};

