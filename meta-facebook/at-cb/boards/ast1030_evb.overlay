};

&fmc {
	status = "okay";
};

&fmc_cs0 {
	status = "okay";
	spi-max-buswidth = <4>;
	spi-max-frequency = <50000000>;

	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;
		boot_partition: partition@0 {
			label = "image-0";
			reg = <0x0 0x100000>;
		};
		dfu_partition: partition@1 {
			label = "image-1";
			reg = <0x0 0x100000>;
		};
	};
};

&fmc_cs1 {
	status = "disabled";
};

&spi1 {
	status = "okay";

	pinctrl-0 = <>;
};

&spi1_cs0 {
	status = "okay";

	spi-max-buswidth = <1>;
	spi-max-frequency = <33000000>;
	re-init-support;
};

&spi1_cs1 {
	status = "disabled";
};

&spi2 {
	status = "okay";

	pinctrl-0 = <&pinctrl_spi2_default>;
