&tach {
	status = "okay";
	pinctrl-0 = <&pinctrl_tach0_default &pinctrl_tach1_default
		&pinctrl_tach2_default &pinctrl_tach3_default
		&pinctrl_tach4_default &pinctrl_tach5_default
		&pinctrl_tach6_default &pinctrl_tach7_default>;

	fan@0 {
		reg = <0>;
		pulse-pr = <2>;
		min-rpm = <1000>;
		tach-div = <5>;
		label = "FAN0";
	};

	fan@1 {
		reg = <1>;
		pulse-pr = <2>;
		min-rpm = <1000>;
		tach-div = <5>;
		label = "FAN1";
	};

	fan@2 {
